{
  "cache_config": {
    "enabled": true,
    "backend": "memory",
    "ttl_seconds": 3600,
    "max_size_mb": 512
  },
  "data_cache": {
    "training_data": {
      "enabled": true,
      "ttl_seconds": 1800,
      "max_entries": 100
    },
    "features": {
      "enabled": true,
      "ttl_seconds": 3600,
      "max_entries": 50
    },
    "predictions": {
      "enabled": true,
      "ttl_seconds": 300,
      "max_entries": 1000
    }
  },
  "model_cache": {
    "enabled": true,
    "model_path": "data/models/",
    "checkpoint_frequency": 10
  },
  "query_cache": {
    "enabled": true,
    "ttl_seconds": 1800,
    "max_queries": 200
  }
}
